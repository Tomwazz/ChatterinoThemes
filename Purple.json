{
  "$schema": "../../docs/ChatterinoTheme.schema.json",
  "metadata": {
    "iconTheme": "light"
  },
  "colors": {
    "accent": "#a64dff",
    "messages": {
      "backgrounds": {
        "alternate": "#2c1b3a",
        "regular": "#1e122b"
      },
      "disabled": "#4d3b6666",
      "highlightAnimationEnd": "#a64dff",
      "highlightAnimationStart": "#d6b3ff",
      "selection": "#7f3fbf",
      "textColors": {
